// { Form, Label, TextField as Input, Submit as Button } as Form

@def CreateUser ( $user )
  <Form
    method="put"
    action=%{ update_user_path }
    for=%{ $user }
    >

    <Label for="name">Nome</Label>
    <TextInput name="name" />
    <br />

    <Label for="email">Email</Label>
    <EmailInput
      placeholder="Digite seu email"
      name="email"
      />
    <br />

    <Submit />
  </Form>
@end

@def Test3 ($props)
  <MaterialApp>
    <CreateUser props=%{ $props } />
  </MaterialApp>
@end

@export default Test3
